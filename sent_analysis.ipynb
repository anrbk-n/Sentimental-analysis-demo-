    "import re\n",
    "\n",
    "def clean_text(text):\n",
    "    text = re.sub(r'<.*?>', '', text) \n",
    "    text = re.sub(r\"[^\\w\\s]\", '', text)\n",
    "    text = text.lower()\n",
    "    text = re.sub(r'\\s+', ' ', text).strip()\n",
    "    \n",
    "    return text\n",
    "\n",
    "train_set['review'] = train_set['review'].apply(clean_text)\n",
    "test_set['review'] = test_set['review'].apply(clean_text)\n",
    "\n"
   ]
